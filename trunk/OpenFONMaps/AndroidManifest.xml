<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.andnav.osm"
      android:versionCode="1"
      android:versionName="1.0.0">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/> 
    <uses-permission android:name="android.permission.INTERNET" />
    <application android:icon="@drawable/open_icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".samples.OpenFonMaps" android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
				<action
					android:name="com.androidMashup.GEO_POINT" />
				<category
					android:name="android.intent.category.DEFAULT" />
			</intent-filter>
			<intent-filter>
				<action
					android:name="com.androidMashup.MAP_VIEW" />
				<category
					android:name="android.intent.category.DEFAULT" />
			</intent-filter>		
        </activity>
    </application>
</manifest> 